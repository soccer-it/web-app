    "next-server": "9.0.5",
    "node-sass": "^4.12.0",
    "now": "^16.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "request": "^2.88.0",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "@nx-js/observer-util": "^4.2.2",
