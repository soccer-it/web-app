  "scripts": {
    "prepare": "node services/getTeams.js",
    "now-build": "yarn run prepare && NODE_ENV=production next build && next export -o dist",
    "now-dev": "yarn run prepare && next -p 5000",
    "dev": "yarn run prepare && next",
